if game.Players.LocalPlayer.Character then
    if game.Players.LocalPlayer.Character:FindFirstChild("Head") then
        if game.Players.LocalPlayer.Character.Head:FindFirstChild("FaceCenterAttachment") then
            game.Players.LocalPlayer.Character.Head:FindFirstChild("FaceCenterAttachment"):destroy()
        end
    end
end
game.Players.LocalPlayer.CharacterAdded:Connect(function(Char)
    repeat wait() until Char 
    repeat wait() until Char:FindFirstChild("Head")
    repeat wait() until Char.Head:FindFirstChild("FaceCenterAttachment")
    Char.Head:FindFirstChild("FaceCenterAttachment"):destroy()
end)

for i,v in pairs(game.Players:GetChildren()) do
    if v ~= game.Players.LocalPlayer then
        if v.Character ~= nil then
            if v.Character:FindFirstChild("Humanoid") then
                if v.Character:FindFirstChild("Head") then
                    if v.UserId == 2910302392 or v.UserId == 886220010 then
                        v.Character.Humanoid.DisplayName = "[⚒️] "..v.Character.Humanoid.DisplayName
                    else
                        if not v.Character.Head:FindFirstChild("FaceCenterAttachment") then
                            v.Character.Humanoid.DisplayName = "[😈] "..v.Character.Humanoid.DisplayName
                        end
                    end
                end
            end
        end
    end
    v.CharacterAdded:Connect(function(Char)
        repeat wait() until Char.Name ~= nil
        if Char.Name ~= game.Players.LocalPlayer then
            repeat wait() until Char 
            repeat wait() until Char:FindFirstChild("Head")
            repeat wait() until Char:FindFirstChild("Humanoid")
            if game.Players:FindFirstChild(Char.Name).UserId == 2910302392 or game.Players:FindFirstChild(Char.Name).UserId == 886220010 then
                Char.Humanoid.DisplayName = "[⚒️] "..Char.Humanoid.DisplayName
            else
                if not Char.Head:FindFirstChild("FaceCenterAttachment") then
                    Char.Humanoid.DisplayName = "[😈] "..Char.Humanoid.DisplayName
                else
                    repeat wait() until not Char.Head:FindFirstChild("FaceCenterAttachment") or Char.Humanoid.Health == 0
                    if Char.Humanoid.Health == 0 then
                    else
                        Char.Humanoid.DisplayName = "[😈] "..Char.Humanoid.DisplayName
                    end
                end
            end
        end
    end)
end

game.Players.PlayerAdded:Connect(function(plr)
    plr.CharacterAdded:Connect(function(Char)
        repeat wait() until Char.Name ~= nil
        if Char.Name ~= game.Players.LocalPlayer then
            repeat wait() until Char 
            repeat wait() until Char:FindFirstChild("Head")
            repeat wait() until Char:FindFirstChild("Humanoid")
            if game.Players:FindFirstChild(Char.Name).UserId == 2910302392 or game.Players:FindFirstChild(Char.Name).UserId == 886220010 then
                Char.Humanoid.DisplayName = "[⚒️] "..Char.Humanoid.DisplayName
            else
                if not Char.Head:FindFirstChild("FaceCenterAttachment") then
                    Char.Humanoid.DisplayName = "[😈] "..Char.Humanoid.DisplayName
                else
                    repeat wait() until not Char.Head:FindFirstChild("FaceCenterAttachment") or Char.Humanoid.Health == 0
                    if Char.Humanoid.Health == 0 then
                    else
                        Char.Humanoid.DisplayName = "[😈] "..Char.Humanoid.DisplayName
                    end
                end
            end
        end
    end)
end)
