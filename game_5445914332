local function GetNearestAnimal()
    local Closest
    local PlayerPosition = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
     
    for i,v in pairs(workspace:GetDescendants()) do
        if v:FindFirstChild("Health") and v:FindFirstChildWhichIsA("MeshPart") and v:FindFirstChild("Humanoid") and not v:FindFirstChild("Body Colors")  then
            if Closest == nil then
                Closest = v:FindFirstChildWhichIsA("MeshPart")
            else
                if (PlayerPosition - v:FindFirstChildWhichIsA("MeshPart").Position).magnitude < (Closest.Position - PlayerPosition).magnitude then
                    Closest = v:FindFirstChildWhichIsA("MeshPart")
                end
            end 
        end
    end
    return Closest.Parent
end
Combat:NewToggle("Animal Aura", "Automaticly Kills Animals", function(state)
    if state then
        getgenv().AutoAnimal = true
        while getgenv().AutoAnimal == true do
            pcall(function()
                NeareastAnimal = GetNearestAnimal()
                Mag = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - NeareastAnimal:FindFirstChildWhichIsA("MeshPart").Position).magnitude
                
                if Mag <= 20 then
                    local args = {
                        [1] = 1,
                        [2] = "Destroyable",
                        [3] = NeareastAnimal 
                    }
                    
                    game:GetService("ReplicatedStorage").References.Comm.Events.ToolAction:FireServer(unpack(args))
                end
            end)
            wait()
        end
    else
        getgenv().AutoAnimal = false
    end
end)
local LocalPlayer = Window:NewTab("Local Player")
local LocalPlayer = LocalPlayer:NewSection("Local Player")
LocalPlayer:NewTextBox("View Player","Views The Player Of The Name u Write",function(txt)
    pcall(function()
    function getFullNameFromPartial(PartialName)
    local CurrentPlayer=nil;
    local Players= game.Players:GetPlayers();
    for i=1,#Players do
     local foundPlayer=Players[i];
    if string.lower(foundPlayer.DisplayName):sub(1,#PartialName)==string.lower(PartialName)then
     CurrentPlayer=foundPlayer.Name;
      break;
    elseif string.lower(foundPlayer.Name):sub(1,#PartialName)==string.lower(PartialName)then
     CurrentPlayer=foundPlayer.Name;
      break;
    end;
    end;
    return CurrentPlayer ;
    end
    
    local CurrentPlayer = getFullNameFromPartial(txt)
    
    game:GetService("Workspace").Camera.CameraSubject = game.Players:FindFirstChild(CurrentPlayer).Character.Humanoid
    end)
end)
LocalPlayer:NewTextBox("Teleport to Player", "Type name of Player to teleport", function(txt)
 function getFullNameFromPartial(PartialName)
local CurrentPlayer=nil;
local Players= game.Players:GetPlayers();
for i=1,#Players do
 local foundPlayer=Players[i];
if string.lower(foundPlayer.DisplayName):sub(1,#PartialName)==string.lower(PartialName)then
 CurrentPlayer=foundPlayer.Name;
  break;
elseif string.lower(foundPlayer.Name):sub(1,#PartialName)==string.lower(PartialName)then
 CurrentPlayer=foundPlayer.Name;
  break;
end;
end;
return CurrentPlayer ;
end

local CurrentPlayer = getFullNameFromPartial(txt)


game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players:FindFirstChild(CurrentPlayer).Character.HumanoidRootPart.CFrame
end)
