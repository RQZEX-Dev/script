
local LocalTycoon = nil

for i, v in pairs(workspace.Tycoons:GetChildren()) do
    if v.Owner.Value == game:GetService("Players").LocalPlayer.Name then
        LocalTycoon = v
    end
end
local SolarisLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/PullingUp/x/main/cc"))()

local win = SolarisLib:New({
  Name = "SkiddHub | Base Raiders",
  FolderToSave = "SkiddHub_BaseRaiders"
})

--win:Tab(title <string>)
local UtilityTab = win:Tab("Utility")
--local TempTab = win:Tab("Temp")
local Utility = UtilityTab:Section("Utility")

Utility:Colorpicker("Color All", Color3.fromRGB(255,255,255),"Colorpicker", function(t)
    for i,v in pairs(LocalTycoon.PlacedContent:GetChildren()) do
        local args = {
            [1] = v,
            [2] = t
        }
        
        game:GetService("ReplicatedStorage").Events.PaintItem:FireServer(unpack(args))
    end
end)
--[[
local sec = TempTab:Section("Elements")

sec:Button("Button", function()
  SolarisLib:Notification("Test", "This is a notification test.")
end)

local toggle = sec:Toggle("Toggle", false,"Toggle", function(t)
  print(t)
end)

local slider = sec:Slider("Slider", 0,25,0,2.5,"Slider", function(t)
  print(t)
end)

local dropdown = sec:Dropdown("Dropdown", {"a","b","c","d","e"},"","Dropdown", function(t)
  print(t)
end)

local multidropdown =sec:MultiDropdown("Multi Dropdown", {"a","b","c","d","e"},{"b", "c"},"Dropdown", function(t)
  print(table.concat(t, ", "))
end)

sec:Textbox("Textbox", true, function(t)
  print(t)
end)

sec:Bind("Hold Bind", Enum.KeyCode.E, true, "BindHold", function(t)
  print(t)
end)

sec:Bind("Normal Bind", Enum.KeyCode.F, false, "BindNormal", function()
  print("Bind pressed")
end)

local label = sec:Label("Label")
]]
